{
  "name": "circulapp-frontend",
  "version": "1.0.0",
  "private": true,
  "description": "Frontend React para CirculApp - Plataforma de EconomÃ­a Circular",
  "homepage": "https://circulapp.com",
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.16",
    "@mui/material": "^5.14.17",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.47.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.17.0",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.7.2",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "analyze": "npm run build && npx bundle-analyzer build/static/js/*.js",
    "serve": "serve -s build -l 3000",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings 0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.8.9",
    "eslint": "^8.52.0",
    "prettier": "^3.0.3",
    "serve": "^14.2.1",
    "eslint-plugin-no-unused-imports": "^1.0.4"
  },
  "keywords": [
    "economia-circular",
    "sustentabilidad",
    "reciclaje",
    "comunidad",
    "react",
    "material-ui"
  ],
  "author": {
